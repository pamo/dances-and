    "gatsby-source-filesystem": "2.0.20",
    "gatsby-source-lastfm": "^0.1.2",
    "gatsby-transformer-remark": "2.2.5",
    "gatsby-transformer-sharp": "2.2.14",
    "js-yaml": "^3.12.1",
    "lodash": "^4.17.21",
    "mkdirp": "^0.5.1",
    "moment": "^2.29.2",
    "prismjs": "^1.24.0",
    "prompt": "^1.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-share": "^2.4.0",
    "react-twitter-widgets": "^1.7.1",
    "react-typography": "^0.16.18",
    "sharp": "^0.21.3",
    "tachyons": "^4.10.0",
    "tachyons-components": "^1.0.1",
    "typography": "^0.16.18",
    "typography-theme-lawton": "^0.16.18",
    "underscore": "^1.12.1",
    "underscore.string": "^3.3.5",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.0",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.1",
    "stylefmt": "^6.0.3",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.0.0",
    "write-good": "^1.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark posts/ && prettier --write posts/**/*.md",
    "write-good": "write-good $(glob 'posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "entry": "node utils/new-post.js"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
